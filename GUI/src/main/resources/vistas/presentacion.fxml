<?import javafx.scene.layout.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<GridPane 
    hgap="10"
    vgap="10"

    xmlns="http://javafx.com/javafx" 
    xmlns:fx="http://javafx.com/fxml"

    >

    <Text text = "hola"/>

    <Text text = "Nueva presentación"
        GridPane.columnIndex="0"
        GridPane.rowIndex="0"
        />

    <Label
        text = "Nombre (*): "
        GridPane.columnIndex="0"
        GridPane.rowIndex="1"
        />

    <TextField
        text = "Nombre presentación"
        GridPane.columnIndex="1"
        GridPane.rowIndex="1"
        />

    <Label
        text = "Expositor: "
        GridPane.columnIndex="0"
        GridPane.rowIndex="2"
        />

    <ComboBox
        promptText = "Expositores"
        GridPane.columnIndex="1"
        GridPane.rowIndex="2"
        />


</GridPane>
